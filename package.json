{
  "name": "website",
  "version": "2.0.0",
  "private": true,
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [ "react", "promise" ],
    "rules": {
        "no-cond-assign": ["error", "always"],
        "linebreak-style": ["error", "unix"],
        "indent": ["warn", 4]
    },
    "env": {}
  },
  "dependencies": {},
  "devDependencies": {
    "gulp": "gulpjs/gulp#4.0",
    "gulp-data": ">=1.2",
    "gulp-swig": ">=0.8",
    "gulp-imagemin": ">= 3.1",
    "gulp-concat": ">=2.6",
    "gulp-less": ">=3.3",
    "gulp-load-plugins": ">=1.4",
    "gulp-cssnano": ">=2.1",
    "gulp-htmlmin": ">=3.0",
    "gulp-uglify": ">=2.0",
    "gulp-rev-all": ">=0.9",
    "gulp-babel": ">=6.1",
    "gulp-util": ">= 3",
    "gulp-changed": ">=1.3",
    "gulp-plumber": ">= 1.1",
    "gulp-task-listing": ">= 1.0",
    "gulp-eslint": ">= 3.0",
    "eslint-plugin-react": ">= 6.9",
    "eslint-plugin-promise": ">= 3.4",
    "babel-preset-es2015": ">= 6.18",
    "babel-preset-react": ">= 6.16",
    "eslint": ">=3.12",
    "colors": ">= 1.1",
    "del": ">=2.2"
  },
  "scripts": {
    "clean": "node scripts/clean.js",
    "gulp": "node node_modules/gulp/bin/gulp.js",
    "build:web": "npm run clean && npm run gulp web:common_js && npm run gulp build:web",
    "build:activity": "npm run clean && npm run gulp activity:common_js && npm run gulp build:activity"
  }
}